/**
message SimpleData {
  string title = 1;
  string description = 2;
  repeated string features = 3;
}
*/

// name: Simple
// param: string title
// param: string description
// param: repeated string features

el {
  div#root.container {
    h1 `title`
    p `description`
    ul {
      each feat `features` {
        li "my `feat`"
      }
    }
  }
}
